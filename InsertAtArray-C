#include<stdio.h>
int main()
{
  int arr[50]={1,2,3,5,6};
  int n=5, pos=3, val=4;
  printf("Original Array:");
  for (int i=0; i<n; i++)
  {
    printf("%d ",arr[i]);
  }
  for (int i=n-1; i>=pos; i--)
  {
    arr[i+1]=arr[i];
  }
  arr[pos]=val;
  n++;
  printf("\nArray after insertion:");
  for (int i=0; i<n; i++)
  {
    printf("%d ",arr[i]);
  }
  return 0;
}
